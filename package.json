{
    "name": "python-comment-sql-syntax",
    "displayName": "python-comment-sql-syntax",
    "description": "syntax of python comment sql ",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.71.0"
    },
    "categories": [
